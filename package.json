    "sb:reset": "cd ./packages/supabase && supabase db reset",
    "sb:apply": "cd ./packages/supabase && supabase migration up",
    "sb:migrate": "cd ./packages/supabase && supabase db diff --schema public --schema internal --schema storage -f"
